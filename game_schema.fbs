namespace GameplayFBData;

table JoinGameRequest {
  player_uuid: string;
  jwt_token: string;
}

union RequestMessages { JoinGameRequest }

table GameRequestEvent {
  msg: RequestMessages;
}

table JoinGameResponseSuccess {
  success: bool;
}

union ResponseMessage { JoinGameResponseSuccess }

table GameResponseEvent {
  msg: ResponseMessage;
}

root_type GameRequestEvent;
